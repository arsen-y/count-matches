{
  "name": "ts-count-matches",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Быстрое решение на TypeScript: подсчёт повторений уникальных элементов A в B, с бенчмарком tinybench и строгим линтингом.",
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "format": "prettier --write .",
    "bench": "tsx bench/bench.ts",
    "examples": "tsx examples/run-examples.ts",
    "test": "tsx --test tests/index.ts"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.3.3",
    "tinybench": "^2.9.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.42.0"
  }
}
